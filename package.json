    "flow:feature:start": "git flow feature start",
    "flow:feature:finish": "git flow feature finish",
    "flow:release:start": "git flow release start",
    "flow:release:finish": "git flow release finish",
    "flow:hotfix:start": "git flow hotfix start",
    "flow:hotfix:finish": "git flow hotfix finish",
